    <Style Name="TabItem" Inherits="ContentControl">
      <ContentStyle>TabItemTextBlock</ContentStyle>
      <Padding>8,3,8,2</Padding>
      <MinWidth>20</MinWidth>
      <Height>22</Height>
      <State Name="Default">
        <Image Margin="0,0,-1,0" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Image="TabItem_Disabled" />
      </State>
      <State Name="MouseOver">
        <Image Margin="0,0,-1,0" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Image="TabItem_MouseOver" />
      </State>
      <State Name="Selected">
        <Image Margin="0,0,-1,0" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Image="TabItem_Selected" />
      </State>
      <State Name="Disabled">
        <Image Margin="0,0,-1,0" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Image="TabItem_Disabled" />
        <Foreground>230,230,230,255</Foreground>
        <Opacity>0.5</Opacity>
      </State>
    </Style>
    <Style Name="TabItemTextBlock" Inherits="TextBlock">
      <UseEllipsis>true</UseEllipsis>
      <VerticalAlignment>Center</VerticalAlignment>
      <State Name="Selected" IsInherited="True">
        <Foreground>0,0,0,255</Foreground>
      </State>
      <State Name="Default">
        <Foreground>230,230,230,255</Foreground>
      </State>
      <State Name="Disabled">
        <Foreground>230,230,230,255</Foreground>
      </State>
    </Style>
    <Style Name="Window" Inherits="ContentControl">
      <Padding>7,32,7,6</Padding>
      <CanDrag>true</CanDrag>
      <CanResize>false</CanResize>
      <CloseButtonStyle>WindowCloseButton</CloseButtonStyle>
      <TitleTextBlockStyle>WindowTitle</TitleTextBlockStyle>
      <ResizeBorder>7,7,7,7</ResizeBorder>
      <IconStyle>WindowIcon</IconStyle>
      <Icon>UITexture</Icon>
      <IconSourceRectangle>Window_IconSourceRectangle</IconSourceRectangle>
      <MinWidth>100</MinWidth>
      <MinHeight>100</MinHeight>
      <State Name="Default">
        <Image Margin="-15,-15,-15,-15" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Image="Window_Default" />
      </State>
      <State Name="Active">
        <Image Margin="-20,-20,-20,-20" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Image="Window_Disabled" />
      </State>
      <State Name="Disabled">
        <Image Margin="-20,-20,-20,-20" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Image="Window_Disabled" />
      </State>
    </Style>
    <Style Name="WindowTitle" Inherits="TextBlock">
      <Margin>32,10,55,0</Margin>
      <UseEllipsis>true</UseEllipsis>
      <State Name="Default">
        <Image Margin="-12,-5,-12,-5" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Image="WindowTitle_Disabled" />
      </State>
      <State Name="Disabled">
        <Image Margin="-12,-5,-12,-5" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Image="WindowTitle_Disabled" />
      </State>
    </Style>
    <Style Name="WindowIcon" Inherits="Image">
      <Width>16</Width>
      <Height>16</Height>
      <Margin>10,9,0,0</Margin>
    </Style>
    <Style Name="WindowCloseButton" Inherits="Button">
      <Width>42</Width>
      <Height>20</Height>
      <HorizontalAlignment>Right</HorizontalAlignment>
      <Margin>0,7,9,0</Margin>
      <ToolTip>Close Window</ToolTip>
      <State Name="MouseOver">
        <Image HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Image="WindowCloseButton_Focused" />
        <Image Name="X" HorizontalAlignment="Center" VerticalAlignment="Center" Image="WindowCloseButton_Default" />
      </State>
      <State Name="Pressed">
        <Image HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Image="WindowCloseButton_Pressed" />
        <Image Name="X" HorizontalAlignment="Center" VerticalAlignment="Center" Image="WindowCloseButton_Default" />
      </State>
      <State Name="Focused">
        <Image HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Image="WindowCloseButton_Focused" />
        <Image Name="X" HorizontalAlignment="Center" VerticalAlignment="Center" Image="WindowCloseButton_Default" />
      </State>
      <State Name="Disabled">
        <Image HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Image="WindowCloseButton_Default" />
        <Image Name="X" HorizontalAlignment="Center" VerticalAlignment="Center" Image="WindowCloseButton_Default" />
      </State>
      <State Name="Active" IsInherited="True">
        <Image HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Image="WindowCloseButton_Active" />
        <Image Name="X" HorizontalAlignment="Center" VerticalAlignment="Center" Image="WindowCloseButton_Default" />
      </State>
      <State Name="Default">
        <Image HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Image="WindowCloseButton_Default" />
        <Image Name="X" HorizontalAlignment="Center" VerticalAlignment="Center" Image="WindowCloseButton_Default" />
      </State>
    </Style>
    <Style Name="InvisibleWindow" Inherits="ContentControl">
      <CloseButtonStyle></CloseButtonStyle>
      <TitleTextBlockStyle></TitleTextBlockStyle>
      <IconStyle></IconStyle>
      <State Name="Default" />
      <State Name="Active" />
      <State Name="Disabled" />
    </Style>
    <!--- TreeView styles for the CustomControlSample-->
    <Style Name="TreeView" Inherits="UIControl">
      <Background>128,128,128,255</Background>
      <State Name="Default" />
      <State Name="Disabled" />
    </Style>
    <Style Name="TreeViewItem" Inherits="UIControl">
      <Padding>20,0,0,0</Padding>
      <State Name="Default"></State>
      <State Name="Expanded">
        <Image Margin="4,6,0,0" Image="TreeViewItem_Expanded" />
      </State>
      <State Name="Collapsed">
        <Image Margin="8,3,0,0" Image="TreeViewItem_Collapsed" />
      </State>
    </Style>
  </Styles>
</Theme>